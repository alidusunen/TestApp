<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Home">

    <Button
            android:text="Add Asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonAdd" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="138dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="86dp"/>
    <Button
            android:text="View Assets"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="136dp" android:layout_marginTop="35dp"
            app:layout_constraintTop_toBottomOf="@+id/buttonAdd"/>

</android.support.constraint.ConstraintLayout>